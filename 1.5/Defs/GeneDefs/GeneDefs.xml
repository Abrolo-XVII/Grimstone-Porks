		<defName>GS_PorkHead</defName>
		<label>pork head</label>
		<labelShortAdj>pork</labelShortAdj>
		<description>Carriers of this gene have a broad pork head.</description>
		<iconPath>UI/Icons/GeneIcons/GS_GenePorkHead</iconPath>
		<biostatMet>1</biostatMet>
		<displayCategory>Cosmetic</displayCategory>
		<displayOrderInCategory>98</displayOrderInCategory>
		<iconColor>(0.75, 0.75, 0.75)</iconColor>
		<forcedHeadTypes>
			<li>GS_PorkHeadF1</li>
			<li>GS_PorkHeadF2</li>
			<li>GS_PorkHeadF3</li>
			<li>GS_PorkHeadF4</li>
			<li>GS_PorkHeadM1</li>
			<li>GS_PorkHeadM2</li>
			<li>GS_PorkHeadM3</li>
			<li>GS_PorkHeadM4</li>
			<li>GS_PorkHeadM5</li>
		</forcedHeadTypes>
		<symbolPack>
			<prefixSymbols>
				<li><symbol>pork</symbol></li>
			</prefixSymbols>
			<suffixSymbols>
				<li><symbol>pork</symbol></li>
			</suffixSymbols>
		</symbolPack>
	</GeneDef>



